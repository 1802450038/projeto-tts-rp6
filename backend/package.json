{
  "name": "tts-project",
  "version": "0.0.1",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/infra/http/server.js",
    "build": "tsc -p tsconfig.build.json && tsc-alias && npm run prisma:push && npm run prisma:generate",
    "dev": "tsx watch src/infra/http/server.ts",
    "test:single": "vitest run",
    "test": "vitest --config vitest.config.ts",
    "seed": "vitest --config vitest.seed.config.ts",
    "prisma:studio": "dotenv -e .env -c -- prisma studio",
    "prisma:generate": "prisma generate dev",
    "prisma:migrate": "dotenv -e .env -c -- prisma migrate dev",
    "prisma:push": "dotenv -e .env -c -- prisma db push",
    "prisma:pull": "dotenv -e .env -c -- prisma db pull",
    "lint": "pnpm biome lint --write",
    "download:model": "npx --no node-llama-cpp pull --dir ./models",
    "list:models": "npx --no node-llama-cpp chat"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
