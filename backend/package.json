    "@aws-sdk/client-polly": "^3.679.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@supabase/supabase-js": "^2.45.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "ffmpeg-static": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "methods": "^1.1.2",
    "multer": "1.4.5-lts.1",
    "node-llama-cpp": "^3.1.1",
    "path": "^0.12.7",
    "pdf-text-reader": "^5.1.0",
    "pdf2json": "^3.1.4",
    "ts-node": "^10.9.2",
    "url": "^0.11.4",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.17.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.13",
